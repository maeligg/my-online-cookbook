  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.8.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "alpinejs": "^2.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maeligg/my-online-cookbook"
  }
